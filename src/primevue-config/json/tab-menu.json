{
  "__id__": "tab-menu",
  "name": "TabMenu",
  "tag": "p-tab-menu",
  "tagIcon": "tabMenu",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/tabmenu",
  "description": "TabMenu is a navigation component that displays items as tab headers.",
  "attributes": {
    "model": {
      "label": "model",
      "title": "An array of menuitems.",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": []
    },
    "active-index": {
      "label": "activeIndex",
      "title": "Active index of menuitem.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 0
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "item",
      "title": "Template of a menuitem."
    },
    {
      "name": "itemicon",
      "title": "Custom item icon template."
    }
  ],
  "events": [
    {
      "name": "tab-change",
      "title": "Callback to invoke when an active tab is changed.",
      "arguments": [
        {
          "name": "event.originalEvent",
          "type": "object",
          "description": "Original event"
        },
        {
          "name": "event.index",
          "type": "number",
          "description": "Index of the selected tab"
        }
      ]
    }
  ]
}