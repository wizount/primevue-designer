{
  "__id__": "chart",
  "name": "Chart",
  "tag": "p-chart",
  "tagIcon": "chart",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/chart",
  "description": "Chart components are based on Charts.js, an open source HTML5 based charting library.",
  "attributes": {
    "type": {
      "label": "type",
      "title": "Type of the chart.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "data": {
      "label": "data",
      "title": "Data to display.",
      "types": [
        {
          "name": "any",
          "tag": "p-input-text"
        }
      ]
    },
    "options": {
      "label": "options",
      "title": "Options to customize the chart.",
      "types": [
        {
          "name": "any",
          "tag": "p-input-text"
        }
      ]
    },
    "plugins": {
      "label": "plugins",
      "title": "Used to custom plugins of the chart.",
      "types": [
        {
          "name": "any",
          "tag": "p-input-text"
        }
      ]
    },
    "width": {
      "label": "width",
      "title": "Width of the chart in non-responsive mode.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 300
    },
    "height": {
      "label": "height",
      "title": "Height of the chart in non-responsive mode.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 150
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [],
  "events": [
    {
      "name": "select",
      "title": "Callback to invoke when a tab gets expanded.",
      "arguments": [
        {
          "name": "originalEvent",
          "type": "object",
          "description": "Browser event"
        },
        {
          "name": "dataset",
          "type": "object",
          "description": "Selected dataset"
        },
        {
          "name": "element",
          "type": "object",
          "description": "Selected element"
        },
        {
          "name": "element._datasetIndex",
          "type": "number",
          "description": "Index of the dataset in data"
        },
        {
          "name": "element._index",
          "type": "number",
          "description": "Index of the data in dataset"
        }
      ]
    },
    {
      "name": "loaded",
      "title": "Callback to invoke when chart is loaded.",
      "arguments": [
        {
          "name": "chart",
          "type": "object",
          "description": "Chart instance."
        }
      ]
    }
  ]
}