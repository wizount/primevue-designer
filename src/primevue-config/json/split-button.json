{
  "__id__": "split-button",
  "name": "SplitButton",
  "tag": "p-split-button",
  "tagIcon": "splitButton",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/splitbutton",
  "description": "SplitButton groups a set of commands in an overlay with a default command.",
  "attributes": {
    "label": {
      "label": "label",
      "title": "Text of the button.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "icon": {
      "label": "icon",
      "title": "Name of the icon.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "model": {
      "label": "model",
      "title": "MenuModel instance to define the overlay items.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "auto-z-index": {
      "label": "autoZIndex",
      "title": "Whether to automatically manage layering.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "base-z-index": {
      "label": "baseZIndex",
      "title": "Base zIndex value to use in layering.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 0
    },
    "append-to": {
      "label": "appendTo",
      "title": "A valid query selector or an HTMLElement to specify where the overlay gets attached.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ],
      "default": "body"
    },
    "disabled": {
      "label": "disabled",
      "title": "When present, it specifies that the element should be disabled.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "class": {
      "label": "class",
      "title": "Style class of the component.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "style": {
      "label": "style",
      "title": "Inline of the component.",
      "types": [
        {
          "name": "any",
          "tag": "style-editor"
        }
      ]
    },
    "menu-button-icon": {
      "label": "menuButtonIcon",
      "title": "Name of the menu button icon.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "severity": {
      "label": "severity",
      "title": "Defines the style of the button, valid values are \"secondary\", \"success\", \"info\", \"warning\", \"help\", \"danger\".",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "secondary",
              "success",
              "info",
              "warning",
              "help",
              "danger"
            ]
          }
        }
      ]
    },
    "raised": {
      "label": "raised",
      "title": "Add a shadow to indicate elevation.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "rounded": {
      "label": "rounded",
      "title": "Add a circular border radius to the button.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "text": {
      "label": "text",
      "title": "Add a textual class to the button without a background initially.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "outlined": {
      "label": "outlined",
      "title": "Add a border class without a background initially.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "size",
      "title": "Defines the size of the button, valid values are \"small\" and \"large\".",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "small",
              "large"
            ]
          }
        }
      ]
    },
    "plain": {
      "label": "plain",
      "title": "Add a plain textual class to the button without a background initially.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "icon",
      "title": "Custom icon template."
    },
    {
      "name": "menubuttonicon",
      "title": "Custom menu button icon template."
    },
    {
      "name": "menuitemicon",
      "title": "Custom menu item icon template."
    }
  ],
  "events": [
    {
      "name": "click",
      "title": "Callback to invoke when main button is clicked.",
      "arguments": [
        {
          "name": "event",
          "type": "object",
          "description": "Browser event"
        }
      ]
    }
  ]
}