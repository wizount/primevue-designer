{
  "__id__": "skeleton",
  "name": "Skeleton",
  "tag": "p-skeleton",
  "tagIcon": "skeleton",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/skeleton",
  "description": "Skeleton is a placeholder to display instead of the actual content.",
  "attributes": {
    "shape": {
      "label": "shape",
      "title": "Shape of the element, options are \"rectangle\" and \"circle\".",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "rectangle",
              "circle"
            ]
          }
        }
      ],
      "default": "rectangle"
    },
    "size": {
      "label": "size",
      "title": "Size of the Circle or Square.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "width": {
      "label": "width",
      "title": "Width of the element.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ],
      "default": "100%"
    },
    "height": {
      "label": "height",
      "title": "Height of the element.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ],
      "default": "1rem"
    },
    "border-radius": {
      "label": "borderRadius",
      "title": "Border radius of the element, defaults to value from theme.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "animation": {
      "label": "animation",
      "title": "Type of the animation, valid options are \"wave\" and \"none\".",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "wave",
              "none"
            ]
          }
        }
      ],
      "default": "wave"
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": []
}