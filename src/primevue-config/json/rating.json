{
  "__id__": "rating",
  "name": "Rating",
  "tag": "p-rating",
  "tagIcon": "rating",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/rating",
  "description": "Rating component is a star based selection input.",
  "attributes": {
    "vModel": "modelValue",
    "disabled": {
      "label": "disabled",
      "title": "When present, it specifies that the element should be disabled.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "readonly": {
      "label": "readonly",
      "title": "When present, it specifies that component is read-only.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "stars": {
      "label": "stars",
      "title": "Number of stars.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 5
    },
    "cancel": {
      "label": "cancel",
      "title": "When specified a cancel icon is displayed to allow clearing the value.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "on-icon": {
      "label": "onIcon",
      "title": "Icon for the on state.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "off-icon": {
      "label": "offIcon",
      "title": "Icon for the off state.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "cancel-icon": {
      "label": "cancelIcon",
      "title": "Icon for the cancelable state.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "cancelicon",
      "title": "Custom cancel icon template."
    },
    {
      "name": "onicon",
      "title": "Custom on icon template."
    },
    {
      "name": "officon",
      "title": "Custom off icon template."
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "Callback to invoke on value change.",
      "arguments": [
        {
          "name": "event.originalEvent",
          "type": "object",
          "description": "Browser event"
        },
        {
          "name": "event.value",
          "type": "number",
          "description": "Selected option value"
        }
      ]
    }
  ]
}