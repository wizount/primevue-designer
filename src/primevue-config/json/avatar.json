{
  "__id__": "avatar",
  "name": "Avatar",
  "tag": "p-avatar",
  "tagIcon": "avatar",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/avatar",
  "description": "Avatar represents people using icons, labels and images.",
  "attributes": {
    "label": {
      "label": "label",
      "title": "Defines the text to display.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "icon": {
      "label": "icon",
      "title": "Defines the icon to display.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "image": {
      "label": "image",
      "title": "Defines the image to display.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "size": {
      "label": "size",
      "title": "Size of the element, valid options are \"large\" and \"xlarge\".",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "large",
              "xlarge"
            ]
          }
        }
      ]
    },
    "shape": {
      "label": "shape",
      "title": "Shape of the element, valid options are \"square\" and \"circle\".",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "square",
              "circle"
            ]
          }
        }
      ],
      "default": "square"
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "icon",
      "title": "Custom icon template."
    }
  ],
  "events": [
    {
      "name": "error",
      "title": "Triggered when an error occurs while loading an image file.",
      "arguments": [
        {
          "name": "event",
          "type": "object",
          "description": "Browser event"
        }
      ]
    }
  ]
}