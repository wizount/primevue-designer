    "welcome-message": {
      "label": "welcomeMessage",
      "title": "Initial text to display on terminal.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "prompt": {
      "label": "prompt",
      "title": "Prompt text for each command.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "pt": {
      "label": "pt",
