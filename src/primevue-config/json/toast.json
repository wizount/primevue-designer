{
  "__id__": "toast",
  "name": "Toast",
  "tag": "p-toast",
  "tagIcon": "toast",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/toast",
  "description": "Toast is used to display messages in an overlay.",
  "attributes": {
    "group": {
      "label": "group",
      "title": "Unique identifier of a message group.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "position": {
      "label": "position",
      "title": "Position of the toast in viewport. Other valid values are \"top-left\", \"top-center\", \"bottom-left\", \"bottom-center\", \"bottom-right\" and \"center\".",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "top-left",
              "top-center",
              "bottom-left",
              "bottom-center",
              "bottom-right",
              "center"
            ]
          }
        }
      ],
      "default": "top-right"
    },
    "auto-z-index": {
      "label": "autoZIndex",
      "title": "Whether to automatically manage layering.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "base-z-index": {
      "label": "baseZIndex",
      "title": "Base zIndex value to use in layering.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 0
    },
    "breakpoints": {
      "label": "breakpoints",
      "title": "Object literal to define widths per screen size.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "message",
      "title": "Custom content for the toast message"
    },
    {
      "name": "icon",
      "title": "Custom icon template."
    },
    {
      "name": "closeicon",
      "title": "Custom close icon template."
    },
    {
      "name": "container",
      "title": "Custom container template."
    }
  ],
  "events": [
    {
      "name": "close",
      "title": "Callback to invoke when the toast is closed.",
      "arguments": [
        {
          "name": "message",
          "type": "any",
          "description": "Message of toast."
        }
      ]
    },
    {
      "name": "life-end",
      "title": "Callback to invoke when the toast timeout is over.",
      "arguments": [
        {
          "name": "message",
          "type": "any",
          "description": "Message of toast."
        }
      ]
    }
  ]
}