{
  "__id__": "chip",
  "name": "Chip",
  "tag": "p-chip",
  "tagIcon": "chip",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/chip",
  "description": "Chip represents entities using icons, labels and images",
  "attributes": {
    "label": {
      "label": "label",
      "title": "Defines the text to display.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "icon": {
      "label": "icon",
      "title": "Defines the icon to display.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "image": {
      "label": "image",
      "title": "Defines the image to display.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "removable": {
      "label": "removable",
      "title": "Whether to display a remove icon.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "remove-icon-class": {
      "label": "removeIconClass",
      "title": "Icon of the remove element.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ],
      "default": "pi pi-times-circle"
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "icon",
      "title": "Custom icon template of chip component."
    },
    {
      "name": "removeicon",
      "title": "Custom remove icon template of chip component."
    }
  ],
  "events": [
    {
      "name": "remove",
      "title": "Callback to invoke when a chip is removed.",
      "arguments": [
        {
          "name": "event",
          "type": "object",
          "description": "Browser event"
        }
      ]
    }
  ]
}