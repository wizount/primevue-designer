{
  "__id__": "editor",
  "name": "Editor",
  "tag": "p-editor",
  "tagIcon": "editor",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/editor",
  "description": "Editor is rich text editor component based on Quill.",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "Please enter ",
    "readonly": {
      "label": "readonly",
      "title": "Whether to instantiate the editor to readonly mode.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "formats": {
      "label": "formats",
      "title": "Whitelist of formats to display.",
      "types": [
        {
          "name": "string[]",
          "tag": "p-input-text"
        }
      ]
    },
    "editor-style": {
      "label": "editorStyle",
      "title": "Inline style of the container.",
      "types": [
        {
          "name": "any",
          "tag": "style-editor"
        }
      ]
    },
    "modules": {
      "label": "modules",
      "title": "Modules configuration, see <a href=\"http://quilljs.com/docs/modules/\">here</a> for available options.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "toolbar",
      "title": "Custom content for the component's toolbar"
    }
  ],
  "events": [
    {
      "name": "text-change",
      "title": "Callback to invoke when text of editor changes.",
      "arguments": [
        {
          "name": "event.delta",
          "type": "object",
          "description": "Representation of the change."
        },
        {
          "name": "event.source",
          "type": "string",
          "description": "Source of change. Will be either \"user\" or \"api\"."
        },
        {
          "name": "event.htmlValue",
          "type": "string",
          "description": "Current value as html."
        },
        {
          "name": "event.textValue",
          "type": "string",
          "description": "Current value as text."
        },
        {
          "name": "event.instance",
          "type": "object",
          "description": "Text editor instance."
        }
      ]
    },
    {
      "name": "selection-change",
      "title": "Callback to invoke when selection of the text changes.",
      "arguments": [
        {
          "name": "event.range",
          "type": "object",
          "description": "Representation of the selection boundaries."
        },
        {
          "name": "event.oldRange",
          "type": "string",
          "description": "Representation of the previous selection boundaries."
        },
        {
          "name": "event.source",
          "type": "string",
          "description": "Source of change. Will be either \"user\" or \"api\"."
        },
        {
          "name": "event.htmlValue",
          "type": "string",
          "description": "Current value as html."
        },
        {
          "name": "event.textValue",
          "type": "string",
          "description": "Current value as text."
        },
        {
          "name": "event.instance",
          "type": "object",
          "description": "Text editor instance."
        }
      ]
    },
    {
      "name": "load",
      "title": "Callback to invoke when the quill modules are loaded.",
      "arguments": [
        {
          "name": "event.instance",
          "type": "any",
          "description": "Quill instance"
        }
      ]
    }
  ]
}