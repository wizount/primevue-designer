{
  "__id__": "avatar",
  "name": "Avatar",
  "tag": "p-avatar",
  "tagIcon": "avatar",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/avatar",
  "description": "Avatar represents people using icons, labels and images.",
  "attributes": {
    "label": {
      "label": "label",
      "title": "Defines the text to display.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "icon": {
      "label": "icon",
      "title": "Defines the icon to display.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "image": {
      "label": "image",
      "title": "Defines the image to display.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "size": {
      "label": "size",
      "title": "Size of the element.",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "normal",
              "large",
              "xlarge"
            ]
          }
        }
      ],
      "default": "normal"
    },
    "shape": {
      "label": "shape",
      "title": "Shape of the element.",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "square",
              "circle"
            ]
          }
        }
      ],
      "default": "square"
    },
    "aria-label": {
      "label": "ariaLabel",
      "title": "Establishes a string value that labels the component.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "aria-labelledby": {
      "label": "ariaLabelledby",
      "title": "Establishes relationships between the component and label(s) where its value should be one or more element IDs.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt-options": {
      "label": "ptOptions",
      "title": "Used to configure passthrough(pt) options of the component.",
      "types": [
        {
          "name": "PassThroughOptions",
          "tag": "p-input-text"
        }
      ]
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "Content can easily be customized with the default slot instead of using the built-in modes."
    },
    {
      "name": "icon",
      "title": "Custom icon template."
    }
  ]
}