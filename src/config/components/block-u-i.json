{
  "__id__": "block-u-i",
  "name": "BlockUI",
  "tag": "p-block-u-i",
  "tagIcon": "blockUI",
  "layouts": [
    "containerItem"
  ],
  "document": "https://primevue.org/blockui",
  "description": "BlockUI represents people using icons, labels and images.",
  "attributes": {
    "blocked": {
      "label": "blocked",
      "title": "Controls the blocked state.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "full-screen": {
      "label": "fullScreen",
      "title": "When enabled, the whole document gets blocked.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "base-z-index": {
      "label": "baseZIndex",
      "title": "Base zIndex value to use in layering.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 0
    },
    "auto-z-index": {
      "label": "autoZIndex",
      "title": "Whether to automatically manage layering.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt-options": {
      "label": "ptOptions",
      "title": "Used to configure passthrough(pt) options of the component.",
      "types": [
        {
          "name": "PassThroughOptions",
          "tag": "p-input-text"
        }
      ]
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "Custom content's slot."
    }
  ]
}