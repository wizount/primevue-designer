{
  "__id__": "menu",
  "name": "Menu",
  "tag": "p-menu",
  "tagIcon": "menu",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/menu/",
  "description": "Menu is a navigation / command component that supports dynamic and static positioning.",
  "attributes": {
    "action": {
      "label": "action",
      "title": "Action element binding",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "icon": {
      "label": "icon",
      "title": "Icon element binding",
      "types": [
        {
          "name": "object",
          "tag": "icon-chooser"
        }
      ],
      "default": {}
    },
    "label": {
      "label": "label",
      "title": "Label element binding",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "model": {
      "label": "model",
      "title": "An array of menuitems.",
      "types": [
        {
          "name": "MenuItem[]",
          "tag": "p-input-text"
        }
      ]
    },
    "popup": {
      "label": "popup",
      "title": "Defines if menu would displayed as a popup.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "append-to": {
      "label": "appendTo",
      "title": "A valid query selector or an HTMLElement to specify where the overlay gets attached.",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "body",
              "self"
            ]
          }
        }
      ],
      "default": "body"
    },
    "auto-z-index": {
      "label": "autoZIndex",
      "title": "Whether to automatically manage layering.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "base-z-index": {
      "label": "baseZIndex",
      "title": "Base zIndex value to use in layering.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 0
    },
    "exact": {
      "label": "exact",
      "title": "Whether to apply 'router-link-active-exact' class if route exactly matches the item path.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "tabindex": {
      "label": "tabindex",
      "title": "Index of the element in tabbing order.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        },
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ]
    },
    "aria-label": {
      "label": "ariaLabel",
      "title": "Defines a string value that labels an interactive element.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "aria-labelledby": {
      "label": "ariaLabelledby",
      "title": "Identifier of the underlying input element.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt-options": {
      "label": "ptOptions",
      "title": "Used to configure passthrough(pt) options of the component.",
      "types": [
        {
          "name": "PassThroughOptions",
          "tag": "p-input-text"
        }
      ]
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "start",
      "title": "Custom start template."
    },
    {
      "name": "end",
      "title": "Custom end template."
    },
    {
      "name": "item",
      "title": "Custom item template."
    },
    {
      "name": "itemicon",
      "title": "Custom item icon template."
    },
    {
      "name": "submenuheader",
      "title": "Custom item template."
    }
  ]
}