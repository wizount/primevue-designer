{
  "__id__": "chip",
  "name": "Chip",
  "tag": "p-chip",
  "tagIcon": "chip",
  "layouts": [
    "containerItem"
  ],
  "document": "https://primevue.org/chip",
  "description": "Chip represents people using icons, labels and images.",
  "attributes": {
    "label": {
      "label": "label",
      "title": "Defines the text to display.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "icon": {
      "label": "icon",
      "title": "Defines the icon to display.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "image": {
      "label": "image",
      "title": "Defines the image to display.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "removable": {
      "label": "removable",
      "title": "Whether to display a remove icon.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "remove-icon": {
      "label": "removeIcon",
      "title": "Icon of the remove element.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt-options": {
      "label": "ptOptions",
      "title": "Used to configure passthrough(pt) options of the component.",
      "types": [
        {
          "name": "PassThroughOptions",
          "tag": "p-input-text"
        }
      ]
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "Content can easily be customized with the default slot instead of using the built-in modes."
    },
    {
      "name": "icon",
      "title": "Custom icon template."
    },
    {
      "name": "removeicon",
      "title": "Custom remove icon template of chip component."
    }
  ]
}