{
  "__id__": "steps",
  "name": "Steps",
  "tag": "p-steps",
  "tagIcon": "steps",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/steps/",
  "description": "Steps components is an indicator for the steps in a wizard workflow. Example below uses nested routes with Steps.",
  "attributes": {
    "action": {
      "label": "action",
      "title": "Action element binding",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "step": {
      "label": "step",
      "title": "Icon element binding",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "label": {
      "label": "label",
      "title": "Label element binding",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "id": {
      "label": "id",
      "title": "Unique identifier of the element.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "model": {
      "label": "model",
      "title": "An array of menuitems.",
      "types": [
        {
          "name": "MenuItem[]",
          "tag": "p-input-text"
        }
      ]
    },
    "readonly": {
      "label": "readonly",
      "title": "Whether the items are clickable or not.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "exact": {
      "label": "exact",
      "title": "Whether to apply 'router-link-active-exact' class if route exactly matches the item path.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "active-step": {
      "label": "activeStep",
      "title": "Active step index of menuitem.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 0
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt-options": {
      "label": "ptOptions",
      "title": "Used to configure passthrough(pt) options of the component.",
      "types": [
        {
          "name": "PassThroughOptions",
          "tag": "p-input-text"
        }
      ]
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "item",
      "title": "Custom item template."
    }
  ]
}