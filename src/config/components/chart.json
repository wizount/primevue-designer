{
  "__id__": "chart",
  "name": "Chart",
  "tag": "p-chart",
  "tagIcon": "chart",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/chart/",
  "description": "Chart groups a collection of contents in tabs.",
  "attributes": {
    "type": {
      "label": "type",
      "title": "Type of the chart.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "data": {
      "label": "data",
      "title": "Data to display.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "options": {
      "label": "options",
      "title": "Options to customize the chart.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "plugins": {
      "label": "plugins",
      "title": "Used to custom plugins of the chart.",
      "types": [
        {
          "name": "any",
          "tag": "p-input-text"
        }
      ]
    },
    "width": {
      "label": "width",
      "title": "Width of the chart in non-responsive mode.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 300
    },
    "height": {
      "label": "height",
      "title": "Height of the chart in non-responsive mode.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 150
    },
    "canvas-props": {
      "label": "canvasProps",
      "title": "Used to pass all properties of the CanvasHTMLAttributes to canvas element inside the component.",
      "types": [
        {
          "name": "CanvasHTMLAttributes",
          "tag": "p-input-text"
        }
      ]
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt-options": {
      "label": "ptOptions",
      "title": "Used to configure passthrough(pt) options of the component.",
      "types": [
        {
          "name": "PassThroughOptions",
          "tag": "p-input-text"
        }
      ]
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": []
}