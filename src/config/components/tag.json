{
  "__id__": "tag",
  "name": "Tag",
  "tag": "p-tag",
  "tagIcon": "tag",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/tag",
  "description": "Tag component is used to categorize content.",
  "attributes": {
    "value": {
      "label": "value",
      "title": "Value to display inside the tag.",
      "types": [
        {
          "name": "any",
          "tag": "p-input-text"
        }
      ]
    },
    "severity": {
      "label": "severity",
      "title": "Severity type of the tag.",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "success",
              "secondary",
              "info",
              "warning",
              "danger",
              "contrast"
            ]
          }
        }
      ]
    },
    "rounded": {
      "label": "rounded",
      "title": "Whether the corners of the tag are rounded.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "icon": {
      "label": "icon",
      "title": "Icon of the tag to display next to the value.",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt-options": {
      "label": "ptOptions",
      "title": "Used to configure passthrough(pt) options of the component.",
      "types": [
        {
          "name": "PassThroughOptions",
          "tag": "p-input-text"
        }
      ]
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "Custom content template"
    },
    {
      "name": "icon",
      "title": "Custom icon template"
    }
  ]
}