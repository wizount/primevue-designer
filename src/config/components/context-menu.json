{
  "__id__": "context-menu",
  "name": "ContextMenu",
  "tag": "p-context-menu",
  "tagIcon": "contextMenu",
  "layouts": [
    "rawItem"
  ],
  "document": "https://primevue.org/contextmenu/",
  "description": "ContextMenu displays an overlay menu on right click of its target. Note that components like DataTable has special integration with ContextMenu.\r\nRefer to documentation of the individual documentation of the with context menu support.",
  "attributes": {
    "action": {
      "label": "action",
      "title": "Action element binding",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "icon": {
      "label": "icon",
      "title": "Icon element binding",
      "types": [
        {
          "name": "object",
          "tag": "icon-chooser"
        }
      ],
      "default": {}
    },
    "label": {
      "label": "label",
      "title": "Label element binding",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "submenuicon": {
      "label": "submenuicon",
      "title": "Submenuicon elemnt binding",
      "types": [
        {
          "name": "object",
          "tag": "icon-chooser"
        }
      ],
      "default": {}
    },
    "model": {
      "label": "model",
      "title": "An array of menuitems.",
      "types": [
        {
          "name": "MenuItem[]",
          "tag": "p-input-text"
        }
      ]
    },
    "append-to": {
      "label": "appendTo",
      "title": "A valid query selector or an HTMLElement to specify where the overlay gets attached.",
      "types": [
        {
          "name": "string",
          "tag": "p-dropdown",
          "props": {
            "options": [
              "body",
              "self"
            ]
          }
        }
      ],
      "default": "body"
    },
    "auto-z-index": {
      "label": "autoZIndex",
      "title": "Whether to automatically manage layering.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "base-z-index": {
      "label": "baseZIndex",
      "title": "Base zIndex value to use in layering.",
      "types": [
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ],
      "default": 0
    },
    "global": {
      "label": "global",
      "title": "Attaches the menu to document instead of a particular item.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    },
    "exact": {
      "label": "exact",
      "title": "Whether to apply 'router-link-active-exact' class if route exactly matches the item path.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": true
    },
    "tabindex": {
      "label": "tabindex",
      "title": "Index of the element in tabbing order.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        },
        {
          "name": "number",
          "tag": "p-input-number"
        }
      ]
    },
    "aria-label": {
      "label": "ariaLabel",
      "title": "Defines a string value that labels an interactive element.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "aria-labelledby": {
      "label": "ariaLabelledby",
      "title": "Identifier of the underlying menu element.",
      "types": [
        {
          "name": "string",
          "tag": "p-input-text"
        }
      ]
    },
    "pt": {
      "label": "pt",
      "title": "Used to pass attributes to DOM elements inside the component.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "pt-options": {
      "label": "ptOptions",
      "title": "Used to configure passthrough(pt) options of the component.",
      "types": [
        {
          "name": "PassThroughOptions",
          "tag": "p-input-text"
        }
      ]
    },
    "unstyled": {
      "label": "unstyled",
      "title": "When enabled, it removes component related styles in the core.",
      "types": [
        {
          "name": "boolean",
          "tag": "p-input-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "item",
      "title": "Custom item template."
    },
    {
      "name": "itemicon",
      "title": "Custom item icon template."
    },
    {
      "name": "submenuicon",
      "title": "Custom submenu icon template."
    }
  ]
}